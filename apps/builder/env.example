# apps/builder environment variables (copy to apps/builder/.env locally)

VITE_API_URL=http://localhost:3000/api

# Public Assets CDN Configuration
# CDN base URL for default assets (images, layouts, music)
# Leave empty to use local assets from /assets/ directory
VITE_PUBLIC_ASSETS_CDN_URL=https://pub-dev.sacredvows.io

# Enable asset caching via Cache API (default: true)
# Set to 'false' to disable caching
VITE_ENABLE_ASSET_CACHING=true

